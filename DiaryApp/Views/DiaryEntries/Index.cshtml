@model List<DiaryEntry>

<div class="table-responsive">
	<table class="table table-striped table-hover">
       
        <thead>
            <!-- set of table rows-thead -->
            <tr>
                <th>Title</th> <!--header cell-->
                <th>Content</th>
                <th>Date</th>
                <th>Actions</th>
     
            </tr>
         </thead>
         <tbody>
             <!--for every item one row-->
             @foreach(var entry in Model){
                 <tr>
                     <td>@entry.Title</td> <!--table data-->
                    <td>@entry.Content</td>
                    <td>@entry.Created</td>
                    <td>Edit and Delete</td>
                </tr>



             }

         </tbody>
         <table>
     <tr>
</div>